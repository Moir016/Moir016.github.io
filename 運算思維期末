        $("#mix").click(function () {
            $("#TwoBar").css("display", "none");
            $("#FourBar").css("display", "none");
            $("#MixBar").css("display", "block");
    
            $("#n0rul3vu").click(function () {//正三角形
                var RGB = $(".one").get(0).style.backgroundColor;
                RGB = rgb2hex(RGB);
    
                if (RGB == "#e42421") {
                    $(".NewOne").css("background-color", "#e42421");
                    $(".NewTwo").css("background-color", "#2b70b0");
                    $(".NewThree").css("background-color", "#f4e600");
                }
                else if (RGB == "#eb621f") {
                    $(".NewOne").css("background-color", "#eb621f");
                    $(".NewTwo").css("background-color", "#434f99");
                    $(".NewThree").css("background-color", "#8dbb25");
                }
                else if (RGB == "#f18e1c") {
                    $(".NewOne").css("background-color", "#f18e1c");
