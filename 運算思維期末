                    $(".TOne").css("background-color", "#6d398b");
                    $(".TTwo").css("background-color", "#c5047c");
                    $(".TThree").css("background-color", "#fdc60c");
                    $(".TFour").css("background-color", "#f4e600");
                }
                else if (RGB == "#c5047c") {
                    $(".TOne").css("background-color", "#c5047c");
                    $(".TTwo").css("background-color", "#e42421");
                    $(".TThree").css("background-color", "#f4e600");
                    $(".TFour").css("background-color", "#8dbb25");
                }
                else if (RGB == "#000000") {
                    $(".TOne").css("background-color", "#000000");
                    $(".TTwo").css("background-color", "#f0f0f0");
                    $(".TThree").css("background-color", "#ffffff");
                    $(".TFour").css("background-color", "#f0f0f0");
                }
                else if (RGB == "#ffffff") {
                    $(".TOne").css("background-color", "#FFFFFF");
                    $(".TTwo").css("background-color", "#f0f0f0");
                    $(".TThree").css("background-color", "#000000");
                    $(".TFour").css("background-color", "#f0f0f0");
                }
            });
    
    
