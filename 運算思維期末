        function rgb2hex(rgb) {
            rgb = rgb.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/); function hex(x) { return ("0" + parseInt(x).toString(16)).slice(-2); } return "#" + hex(rgb[1]) + hex(rgb[2]) + hex(rgb[3]);
        }
    
    
        $("#one").click(function () {
    
            $("#TwoBar").css("display", "none");
            $("#MixBar").css("display", "none");
            $("#FourBar").css("display", "none");
    
            var RGB = $(".one").get(0).style.backgroundColor;
            RGB = rgb2hex(RGB);
    
            if (RGB == "#e42421") {
                $(".two").css("background-color", "#ff1618");
            }
            else if (RGB == "#eb621f") {
                $(".two").css("background-color", "#ff6a12");
            }
            else if (RGB == "#f18e1c") {
                $(".two").css("background-color", "#ffa200");
            }
            else if (RGB == "#fdc60c") {
                $(".two").css("background-color", "#ffe100");
            }
            else if (RGB == "#f4e600") {
                $(".two").css("background-color", "#feff00");
            }
            else if (RGB == "#8dbb25") {
                $(".two").css("background-color", "#a0d610");
            }
            else if (RGB == "#008e5a") {
                $(".two").css("background-color", "#00a260");
            }
            else if (RGB == "#0896bb") {
                $(".two").css("background-color", "#00abd4");
            }
            else if (RGB == "#2b70b0") {
                $(".two").css("background-color", "#207cc9");
            }
            else if (RGB == "#434f99") {
                $(".two").css("background-color", "#4451ae");
            }
            else if (RGB == "#6d398b") {
                $(".two").css("background-color", "#76399e");
            }
            else if (RGB == "#c5047c") {
                $(".two").css("background-color", "#e0008c");
            }
            else if (RGB == "#000000") {
                $(".two").css("background-color", "#8E8E8E");
            }
            else if (RGB == "#ffffff") {
                $(".two").css("background-color", "#8E8E8E");
            }
    
        });
    
        $("#two").click(function () {
    
            $("#MixBar").css("display", "none");
            $("#FourBar").css("display", "none");
            $("#TwoBar").css("display", "block");
    
            $("#2jo4au04nk4").click(function () { //對面色
